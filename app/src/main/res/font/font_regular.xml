<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font android:fontStyle="normal"
        android:font="@font/roboto_regular"
        app:fontStyle="normal"
        app:font="@font/roboto_regular"
        tools:targetApi="o" />

    <font android:fontStyle="italic"
        android:font="@font/roboto_italic"
        app:fontStyle="italic"
        app:font="@font/roboto_italic"
        tools:targetApi="o" />
</font-family>

